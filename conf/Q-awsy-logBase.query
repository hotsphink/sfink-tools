# Processed with `artifetch --query <thisfile>`

jobs:
  symbol: "SY(ab)"
  limit-per-push: 1

artifact: gecko.log

metric:
  text:
    match-key: '/STRSTAT ([-\d]+): ([-\d]+) ([-\d]+)/'
    value: "$3"
    label:
      pid: "$1"  # Hm... this is colliding syntax!
      old: "$2"
  output:
    style: formatted
    header-format: "# job {job_desc} on push: {push_desc}\n# {push_url}"
    format: "{push_idx} {pid} {sum(old)} {sum(value)}"
    groupby: ["push", "pid"]
